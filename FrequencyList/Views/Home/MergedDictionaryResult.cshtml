@model List<KeyValuePair<string, int>>

<h2>Об'єднаний частотний словник</h2>

<table>
    <thead>
        <tr>
            <th>Слово</th>
            <th>Частота</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var entry in Model)
        {
            <tr>
                <td>@entry.Key</td>
                <td>@entry.Value</td>
            </tr>
        }
    </tbody>
</table>

<a href="/Home/Index">Назад</a>
